{
  "name": "eboto-mo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "pnpm with-env turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:push": "pnpm with-env turbo --filter @eboto-mo/db db:push",
    "with-env": "dotenv -e .env --"
  },
  "devDependencies": {
    "dotenv-cli": "^7.2.1",
    "@turbo/gen": "^1.10.7",
    "eslint": "^8.45.0",
    "@eboto-mo/eslint-config": "workspace:*",
    "prettier": "^3.0.0",
    "turbo": "^1.10.7"
  },
  "packageManager": "pnpm@7.15.0"
}
