{
  "name": "eboto-mo",
  "private": true,
  "scripts": {
    "build": "turbo run build --no-daemon",
    "dev": "pnpm with-env turbo run dev --no-daemon",
    "start": "pnpm with-env turbo run start --no-daemon",
    "lint": "turbo run lint",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:push": "pnpm with-env turbo --filter @eboto-mo/db db:push",
    "db:studio": "pnpm with-env turbo --filter @eboto-mo/db db:studio",
    "db:generate": "pnpm with-env turbo --filter @eboto-mo/db db:generate",
    "with-env": "dotenv -e .env --"
  },
  "devDependencies": {
    "@eboto-mo/eslint-config": "workspace:*",
    "@eboto-mo/tsconfig": "workspace:*",
    "@turbo/gen": "^1.10.11",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "turbo": "^1.10.12"
  },
  "packageManager": "pnpm@8.6.7"
}
