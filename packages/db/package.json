{
  "name": "@eboto/db",
  "version": "0.0.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "db:push": "pnpm with-env drizzle-kit push:pg -- --verbose",
    "db:studio": "pnpm with-env drizzle-kit studio --verbose",
    "db:seed": "pnpm with-env tsx ./seed.ts",
    "db:generate": "pnpm with-env drizzle-kit generate:pg",
    "clean": "rm -rf .turbo node_modules",
    "with-env": "dotenv -e ../../.env.local --"
  },
  "dependencies": {
    "@auth/core": "^0.27.0",
    "@neondatabase/serverless": "^0.9.0",
    "drizzle-orm": "0.30.1",
    "nanoid": "^5.0.6",
    "tsx": "^4.7.1",
    "uploadthing": "^6.4.1"
  },
  "devDependencies": {
    "@eboto/eslint-config": "0.0.0",
    "@eboto/prettier-config": "0.0.0",
    "@eboto/tsconfig": "0.0.0",
    "@types/node": "^20.11.20",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "0.20.14",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@eboto/eslint-config/base"
    ]
  },
  "prettier": "@eboto/prettier-config"
}
